{
  "family": "apiserver",
  "executionRoleArn": "arn:aws:iam::228655407122:role/github-ecs-deploy-roy",
  "networkMode": "awsvpc",
  "requiresCompatibilities": ["FARGATE"],
  "cpu": "256",
  "memory": "512",
  "containerDefinitions": [
    {
      "name": "itstudy",
      "image": "IMAGE_URI",
      "portMappings": [
        {
          "containerPort": 80,
          "protocol": "tcp"
        }
      ],
      "essential": true
    }
  ]
}
